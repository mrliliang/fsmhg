    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-Test<FSMHG>",
            "request": "launch",
            "mainClass": "Test",
            "projectName": "FSMHG"
        },
        {
            "type": "java",
            "name": "Launch-Main<FSMHG>",
            "request": "launch",
            "mainClass": "com.liang.fsmhg.Main",
            "projectName": "FSMHG",
            "cwd": "${workspaceFolder}",
            "args": ["-d", "test", "-s", "0.04", "-o", "out/test_result"]
        },
        {
            "type": "java",
            "name": "Launch-Main<FSMHGWIN>",
            "request": "launch",
            "mainClass": "com.liang.fsmhg.Main",
            "projectName": "FSMHG",
            "cwd": "${workspaceFolder}",
            "args": ["-d", "test", "-s", "0.04", "-w", "9000", "-ss", "100", "-e", "1"]
        },
        {
            "type": "java",
            "name": "Debug (Launch)-Generator<FSMHG>",
            "request": "launch",
            "mainClass": "com.liang.fsmhg.graph.Generator",
            "projectName": "FSMHG"
        }
    ]
}